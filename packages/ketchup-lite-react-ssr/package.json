{
  "name": "ketchup-lite-react-ssr",
  "version": "2.3.0-rc.5",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "keywords": [
    "components",
    "custom elements",
    "ketchup lite",
    "pwa",
    "ssr",
    "ssg",
    "react",
    "react components"
  ],
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "node -e \"require('fs').rmSync('./dist', { recursive: true, force: true })\" && node -e \"require('fs').rmSync('.components.ts', { recursive: true, force: true })\" && node -e \"require('fs').rmSync('.components.server.ts', { recursive: true, force: true })\""
  },
  "dependencies": {
    "@stencil/react-output-target": "^0",
    "ketchup-lite": "^2.3.0-rc.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/react": "^18",
    "@types/react-dom": "^18"
  },
  "description": "Ketchup Lite - React Components library (SSR)",
  "license": "Apache-2.0",
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
