{
    "name": "ketchup-lite",
    "version": "0.0.1",
    "keywords": [
        "ketchup lite",
        "web component",
        "library",
        "Stencil",
        "pwa",
        "custom elements",
        "components"
    ],
    "description": "Web components library",
    "license": "Apache-2.0",
    "main": "./dist/index.cjs.js",
    "module": "dist/index.js",
    "es2015": "dist/esm/index.mjs",
    "es2017": "dist/esm/index.mjs",
    "unpkg": "dist/ionic.js",
    "types": "dist/types/components.d.ts",
    "collection": "dist/collection/collection-manifest.json",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build": "rimraf target && stencil build --docs",
        "dev": "rimraf target && stencil build --dev --watch --serve",
        "test:spec": "rimraf target && stencil test --spec",
        "test:speccov": "rimraf target && stencil test --spec --coverage",
        "test:e2e": "rimraf target && stencil test --e2e",
        "test": "rimraf target && stencil test --spec --e2e",
        "test.watch": "rimraf target && stencil test --spec --e2e --watchAll"
    },
    "dependencies": {
        "@fullcalendar/core": "^6.1.8",
        "@fullcalendar/daygrid": "^6.1.8",
        "@fullcalendar/interaction": "^6.1.8",
        "@fullcalendar/list": "^6.1.8",
        "@fullcalendar/timegrid": "^6.1.8",
        "dayjs": "^1.11.10",
        "echarts": "^5.4.2",
        "html2canvas": "^1.4.1",
        "numeral": "^2.0.6"
    },
    "devDependencies": {
        "@stencil/core": "^4.14.1",
        "@stencil/react-output-target": "0.5.3",
        "@stencil/sass": "^3.0.11",
        "jest": "^29.7.0",
        "jest-cli": "^29.7.0",
        "jest-html-reporter": "^3.10.2",
        "jest-junit": "^16.0.0",
        "puppeteer": "^21.6.0",
        "rimraf": "^5.0.5"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lucafoscili/ketchup-lite.git"
    },
    "installConfig": {
        "hoistingLimits": "workspaces"
    }
}
